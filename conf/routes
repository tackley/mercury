# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET     /uknf                       controllers.Application.today
GET     /uknf/yesterday             controllers.Application.yesterday
GET     /uknf/:year/:month/:day     controllers.Application.day(year: Int, month: Int, day: Int)

GET     /uknf/slide/:year/:month/:day   controllers.Application.slide(year: Int, month: Int, day: Int)

GET     /slide-demo     controllers.Application.slideDemo