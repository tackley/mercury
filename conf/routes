# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /assets/*file               controllers.Assets.at(path="/public", file)

#GET     /                           controllers.Application.index
#GET     /:pageName                   controllers.Application.index
#GET     /:pageName/:year                      controllers.Application.months(year: Int)
#GET     /:pageName/:year/:month               controllers.Application.days(year: Int, month: Int)
GET     /uknf                       controllers.Application.today
GET     /uknf/:year/:month/:day          controllers.Application.day(year: Int, month: Int, day: Int)

